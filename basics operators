course=input("enter course:")
college=input("enter college")
year=int(input("enter year:"))
backlogs=int(input("bc"))
branch=input("branch")
gender=input("gen")
percentage=int(input("per"))
if(course=="btech" and college=="aec" and year==4 and backlogs==0 and percentage>=65 and( branch=="cse" or branch=="it")and gender=='M'):
    print("yes")  
else:
    print("No")
#functions
def add(x,y=1):
    c=x+y
    return c
    
a=int(input("enter a value"))
b=int(input("enter b value"))
res=add(a,b)
print(res)
